using System;

class Program
{
    static void Main(string[] args)
    {
        Console.Write("Ola, mundo \n");
        Console.Write("Hello, Word \t");
        Console.WriteLine("Ola, mundo");
        Console.WriteLine("Hello, Word");

        string nome = "Patrick";
        string nome2 ="Caua";
        Console.WriteLine(nome);

        if (nome2 == "Patrick")
        {
            Console.WriteLine(nome2);
        }
        else
            {
                Console.WriteLine(nome2);
            }
    }
} 
